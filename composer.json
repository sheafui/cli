{
  "name": "sheaf/cli",
  "description": "A CLI tool for Sheaf UI",
  "type": "library",
  "keywords": [
    "laravel",
    "cli",
    "sheaf",
    "package",
    "laravel-package",
    "artisan-command"   
  ],
  "homepage": "https://github.com/sheaf/cli",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Sheaf\\Cli\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Sheaf\\Cli\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Youssef ACHCHIRAJ",
      "email": "achchiraj.youssef@gmail.com",
      "homepage": "https://youssef-achchiraj.com",
      "role": "Full Stack Developer"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "Sheaf\\Cli\\ServiceProvider"
      ]
    }
  },
  "require-dev": {
    "orchestra/testbench": "^10.4",
    "pestphp/pest": "^3.8",
    "pestphp/pest-plugin-laravel": "^3.2"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "scripts": {
    "test": "@php vendor/bin/pest"
  }
}
